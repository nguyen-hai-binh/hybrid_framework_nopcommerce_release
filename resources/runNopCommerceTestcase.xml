<?xml version="1.0" encoding="UTF-8"?>
<suite name="NopCommerce">
	
	<test name="Run on Chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.nopcommerce.users.Nopcommerce_Register" />
			<class name="com.nopcommerce.users.Nopcommerce_Login" />
		</classes>
	</test>
	
	<test name="Run on Firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.nopcommerce.users.Nopcommerce_01_Register" />
			<class name="com.nopcommerce.users.Nopcommerce_01_Login" />
		</classes>
	</test>
	
	<test name="Run on Edge">
		<parameter name="browser" value="Edge" />
		<classes>
			<class name="com.nopcommerce.users.Nopcommerce_01_Register" />
			<class name="com.nopcommerce.users.Nopcommerce_01_Login" />
		</classes>
	</test>
	
</suite>
